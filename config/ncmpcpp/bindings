def_key "h"
    seek_backward
def_key "l"
    seek_forward

def_key "j"
    scroll_down
def_key "k"
    scroll_up

def_key "G"
    move_end
def_key "g"
    move_home

def_key "J"
    page_down
def_key "K"
    page_up

def_key "f"
    push_character "enter"

def_key "="
    volume_up

def_key "z"
    previous
def_key "b"
    next
def_key "c"
    pause
def_key "x"
    pause

def_key "R"
    toggle_random
def_key "O"
    toggle_consume
def_key "W"
    shuffle

def_key "n"
    next_found_item
def_key "N"
    previous_found_item

def_key "L"
    show_lyrics

def_key "C"
    clear_main_playlist
def_key "d"
    delete_playlist_items

def_key "."
    move_selected_items_down
def_key ","
    move_selected_items_up

def_key "q"
    jump_to_parent_directory
def_key "Q"
    quit


